{
  "name": "nile_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "node-sass-chokidar": "0.0.3",
    "re-bulma": "^0.4.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-iframe": "^1.0.7",
    "react-scripts": "1.0.13"
  },
  "scripts": {
